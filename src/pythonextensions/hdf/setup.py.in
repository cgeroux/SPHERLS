from distutils.core import setup, Extension

module1 = Extension('hdf', sources = ['hdfmodule.cpp'], include_dirs=['@HDF_INCLUDE_PATH@']
  , libraries=['m','mfhdf','df','jpeg','z'], library_dirs=['@HDF_LIB_PATH@'])
 
setup (name = 'PackageName',
        version = '1.0',
        description = 'This is a python wrapper for c++ hdf 4 library',
        ext_modules = [module1])